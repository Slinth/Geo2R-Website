# BEGIN Cleaning URLs
<IfModule mod_rewrite.c>
    RewriteEngine On
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_URI} !^pages/
	RewriteRule ^(.*)$ /pages/$1 [L,QSA]
</IfModule>

# END Cleaning URLs

